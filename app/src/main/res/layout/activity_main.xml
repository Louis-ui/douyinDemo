<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.activity.MainActivity">

        <Button
            android:id="@+id/btn_getAccessToken"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="auth info"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_goMovieRankList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_getAccessToken"
            android:text="movie rank" />

        <TextView
            android:id="@+id/client_token"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintBottom_toTopOf="@+id/btn_getAccessToken"
            app:layout_constraintEnd_toEndOf="@+id/btn_getAccessToken"
            app:layout_constraintStart_toStartOf="@+id/btn_getAccessToken" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>